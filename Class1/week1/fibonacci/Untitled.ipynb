    "# Uses python3\n",
    "\n",
    "def fib(n):\n",
    "    F = {}\n",
    "    def _fib(n, F):\n",
    "        if n in F:\n",
    "            return F[n]\n",
    "        elif n <= 1:\n",
    "            return n\n",
    "        else:\n",
    "            F[n] = _fib(n-1, F)+_fib(n-2, F)\n",
    "            return F[n]\n",
    "    return _fib(n,F)"
   ]
